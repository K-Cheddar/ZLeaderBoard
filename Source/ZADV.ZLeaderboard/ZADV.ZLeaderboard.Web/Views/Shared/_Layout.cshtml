    <script src="~/App/admin/authController.js"></script>
    <script src="~/App/admin/eventController.js"></script>
    <script src="~/App/admin/eventService.js"></script>
    <script src="~/App/admin/mainController.js"></script>
    <script src="~/App/user/userEventController.js"></script>
    <script src="~/App/user/userHomeController.js"></script>
    <script src="~/App/user/userEventService.js"></script>
