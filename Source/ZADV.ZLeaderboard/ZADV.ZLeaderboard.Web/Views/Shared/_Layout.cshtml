    <script src="~/App/app.js"></script>
    <script src="~/App/vendors/angular-cookies.js"></script>
    <script src="~/App/vendors/angular-route.js"></script>
    <script src="~/App/vendors/truncate.js"></script>
    <script src="~/App/vendors/datetimepicker.js"></script>
